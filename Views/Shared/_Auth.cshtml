<script>
    $(document).ready(function () {
        $("#btnLogin").click(function () {
            $("#modalLogin").modal('show');
        });
        $("#btnSignup").click(function () {
            $("#modalRes").modal('show');
        });
        $("#buttonRes").click(function () {
            $("#modalLogin").modal('hide');
            $("#modalRes").modal('show');
        });
        $("#buttonLog").click(function () {
            $("#modalRes").modal('hide');
            $("#modalLogin").modal('show');
        });

        $("#loginFb").click(function () {
            $("#modalLogin").modal('show');
        });


        $('#dropdownBtn').on('click', function (e) {
            e.preventDefault();
            $('#dropdownMenu').toggleClass('show');
        });

        $(document).on('click', function (e) {
            if (!$(e.target).closest('#dropdownBtn').length) {
                $('#dropdownMenu').removeClass('show');
            }
        });
        $('.menu--moblie').hide();

        function toggleMenu() {
            $('.menu--moblie').toggleClass('show');
            $('.tog-main').toggleClass('fa-bars fa-times');
        }

        $('.tog-main, .fas.fa-times').click(function () {
            toggleMenu();
        });

        $(document).click(function (e) {
            if (!$(e.target).closest('.menu--moblie').length && !$(e.target).hasClass('tog-main')) {
                $('.menu--moblie').removeClass('show');
                $('.tog-main').removeClass('fa-times').addClass('fa-bars');
            }
        });

        $('.toggle-icon').click(function () {
            $('.main-menu-mobile').toggleClass('activeMain');
        });
        $('.main-menu-mobile .grades').click(function (e) {
            $('.main-menu-mobile').removeClass('activeMain');
        });
        $(document).click(function (e) {
            if (!$(e.target).closest('.mainMobile').length) {
                $('.main-menu-mobile').removeClass('activeMain');
            }
        });
        $(".toggle-password").click(function () {
            $(this).toggleClass("fa-eye fa-eye-slash");
            input = $(this).parent().find("input");
            if (input.attr("type") == "password") {
                input.attr("type", "text");
            } else {
                input.attr("type", "password");
            }
        });
    })

</script>
<script asp-append-version="true">
    var navbar_vue = new Vue({
        el: '#navbar_vue',
        data: {
            userName: '',
            optionRole: '',
            FullName: '',
            PhoneNumber: '',
            password: '',
            ConfirmPassword: '',
            mess: null,
            PriceUser: 0,
            userCheck: false,
            RememberMe: true,
            showPassword: false,
            showRePassword: false,
            showMainPassword: false,
            Email: "",

        },
        computed: {
            inputType() {
                return this.showPassword ? 'text' : 'password';
            },
            reType() {
                return this.showRePassword ? 'text' : 'password';
            },
            mainType() {
                return this.showMainPassword ? 'text' : 'password';
            }
        },
        methods: {
            togglePasswordVisibility() {
                this.showPassword = !this.showPassword;
            },
            togglePasswordRe() {
                this.showRePassword = !this.showRePassword;
            },
            togglePassword() {
                this.showMainPassword = !this.showMainPassword;
            },
            convertToSlug(text) {
                return text
                    .toLowerCase()
                    .normalize("NFD")
                    .replace(/[\u0300-\u036f]/g, "")
                    .replace(/\s+/g, "-")
                    .replace(/[^\w-]+/g, "");
            },
            async postLogin() {
                $('#loadingMore').fadeIn();

                const formData = new FormData();

                formData.append('UserName', this.userName);
                formData.append('Password', this.password);
                formData.append('RememberMe', this.RememberMe);
                await axios.post('/tai-khoan/dang-nhap', formData)
                    .then(res => {
                        if (res.data.code == 200) {
                            localStorage.removeItem("checkRes");
                            localStorage.setItem('checkLog', res.data.section);
                            window.location.reload();
                        }
                        else if (res.data.code == 208) {
                            const hef = res.data.red;
                            window.location.href = hef;
                        }
                        else if (res.data.code == 408) {
                            this.mess = res.data.message;
                            console.log(this.mess);

                        } else {
                            this.mess = res.data.message;
                            console.log(this.mess);

                        }
                    }).finally(() => {
                        $('#loadingMore').fadeOut();
                    });
            },
            async postRes() {
                $('#loadingMore').fadeIn();

                const formData = new FormData();
                formData.append('PasswordHash', this.password);
                formData.append('ConfirmPassword', this.ConfirmPassword);
                formData.append('FullName', this.FullName);
                formData.append('UserName', this.userName);
                formData.append('PhoneNumber', this.PhoneNumber);
                formData.append('Email', this.Email);
                formData.append('OptionRole', this.optionRole);
                await axios.post('/tai-khoan/dang-ky-tai-khoan', formData)
                    .then(res => {
                        if (res.data.code == 200) {
                            localStorage.setItem('checkRes', res.data.section);
                            $("#modalRes").modal('hide');
                            $("#modalLogin").modal('show');
                        }
                        else {
                            this.mess = res.data.message;
                            console.log(this.mess);
                        }
                    }).finally(() => {
                        $('#loadingMore').fadeOut();
                    });
            },
            Logout() {
                $('#loadingMore').fadeIn();

                fetch("/dang-xuat", {
                    method: "GET"
                })
                    .then(res => {
                        localStorage.removeItem("checkRes");
                        localStorage.removeItem('checkLog');
                        window.location.reload()
                    })
            }
        }
    });
</script>




